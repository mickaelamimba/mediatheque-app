{
  "name": "mediatheque-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "heroku-postbuild": "cd client && yarn install && yarn install --only=dev --no-shrinkwrap && yarn run build",
    "start": "node ./bin/www",
    "server": "nodemon ./bin/www",
    "client": "yarn start --prefix client",
    "dev": "concurrently \" yarn server \" \"yarn client\""
  },
  "dependencies": {
    "compression": "^1.7.4",
    "concurrently": "^6.3.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "express-fileupload": "^1.2.1",
    "helmet": "^4.6.0",
    "mongoose": "^6.0.10",
    "mongoose-fuzzy-searching": "^2.0.2",
    "mongoose-paginate-v2": "^1.4.2",
    "mongoose-type-email": "^1.1.2",
    "nodemon": "^2.0.13",
    "sharp": "^0.29.1",
    "uid2": "^1.0.0",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": "14.x"
  }
}
